@model StoreCore.Data.ApplicationUser

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<h1>Detalhes do</h1>
<h4>Usuário</h4>
<hr />

<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item active"><a asp-action="index">Voltar</a></li>
        <li class="breadcrumb-item"><a asp-action="index" asp-controller="Admin">Painel</a></li>
    </ol>
</nav>


<table class="table">

    <tbody>
        <tr>
            <th>@Html.LabelForModel("Usuário")</th>
            <td>@Html.DisplayFor(model => model.UserName)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Nome de usuário normalizado")</th>
            <td>@Html.DisplayFor(model => model.NormalizedUserName)</td>
        </tr>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Email)</th>
            <td>@Html.DisplayFor(model => model.Email)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("E-mail normalizado")</th>
            <td>@Html.DisplayFor(model => model.NormalizedEmail)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Email confirmado")</th>
            <td>@Html.DisplayFor(model => model.EmailConfirmed)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Senha hash")</th>
            <td>@Html.DisplayFor(model => model.PasswordHash)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Carimbo de segurança")</th>
            <td>@Html.DisplayFor(model => model.SecurityStamp)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Carimbo de Concorrência")</th>
            <td>@Html.DisplayFor(model => model.ConcurrencyStamp)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Número de telefone")</th>
            <td>@Html.DisplayFor(model => model.PhoneNumber)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Número de telefone confirmado")</th>
            <td>@Html.DisplayFor(model => model.PhoneNumberConfirmed)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Dois fatores habilitados")</th>
            <td>@Html.DisplayFor(model => model.TwoFactorEnabled)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Fim do bloqueio")</th>
            <td>@Html.DisplayFor(model => model.LockoutEnd)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Bloqueio ativado")</th>
            <td>@Html.DisplayFor(model => model.LockoutEnabled)</td>
        </tr>
        <tr>
            <th>@Html.LabelForModel("Contagem de falhas no acesso")</th>
            <td>@Html.DisplayFor(model => model.AccessFailedCount)</td>
        </tr>
    </tbody>
</table>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar</a> |
    <a asp-action="Index" class="btn btn-secondary">Voltar</a>
</div>
