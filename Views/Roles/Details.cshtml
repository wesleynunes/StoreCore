@model StoreCore.Data.ApplicationRole

@{
    ViewData["Title"] = "Detalhes";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<h1>Detalhes do</h1>
<h4>papel</h4>
<hr />

<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item active"><a asp-action="index">Voltar</a></li>
        <li class="breadcrumb-item"><a asp-action="index" asp-controller="Admin">Painel</a></li>
    </ol>
</nav>

<div>    
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.LabelForModel("Nome")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.LabelForModel("Nome normalizado")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NormalizedName)
        </dd>
        <dt class="col-sm-2">
            @Html.LabelForModel("Carimbo de Concorrência")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ConcurrencyStamp)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar</a> |
    <a asp-action="Index" class="btn btn-secondary">Voltar</a>
</div>
